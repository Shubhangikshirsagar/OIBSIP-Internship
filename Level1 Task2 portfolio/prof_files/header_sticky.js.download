DT.onReady(()=>{const header=document.querySelector('.js-header');const headerSearch=document.querySelector('.js-header-search');new IntersectionObserver((entries)=>{if(entries.length){const{intersectionRect}=entries[0];if(intersectionRect.y>0&&!DT.UI.is(header,'sticky')){return}}
DT.UI.toggle(header,'sticky')},{threshold:1.0},).observe(headerSearch)})